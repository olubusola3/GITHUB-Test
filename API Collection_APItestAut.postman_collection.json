							"\r",
							"\r",
							"pm.test(\"Status code should return 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Status code name should have  an OK string\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							"\r",
							"console.log(jsonData)\r",
							"console.log(jsonData.totalResults)\r",
							"console.log(jsonData.entry[14].title)\r",
							"console.log(jsonData.entry[7].status)\r",
							"console.log(jsonData.entry[10].id)\r",
							"\r",
							"pm.test(\"Confirm that the totalResults is 31\", function () {\r",
							"        pm.expect(jsonData.totalResults).to.eql(\"31\");\r",
							"});\r",
							"\r",
							"pm.test(\"Confirm that the title for this\", function () {\r",
							"        pm.expect(jsonData.entry[14].title).to.eql(\"Task 15\");\r",
							"});\r",
							"\r",
							" pm.test(\"The string title Task 2 is in response body\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"Task 15\");\r",
							"});\r",
							"\r",
							"pm.test(\"Confirm that the Status is 1 for Task 2\", function () {\r",
							"        pm.expect(jsonData.entry[7].status).to.eql(\"1\");\r",
							"});\r",
							"pm.test(\"Confirm that the entry 10 has an id equal 11\", function () {\r",
							"        pm.expect(jsonData.entry[10].id).to.eql(\"11\");\r",
							"});\r",
							"\r",
							""
