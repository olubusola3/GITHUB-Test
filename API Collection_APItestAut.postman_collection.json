				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"console.log(jsonData.entry[10].id);\r",
							"console.log(jsonData.entry[14].title)\r",
							"\r",
							"pm.environment.set(\"entityId\", jsonData.entry[10].id);\r",
